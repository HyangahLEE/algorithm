package Main;

import java.util.Scanner;

public class main01 {

	public static void main(String[] args) {
		int m[]= new int[5];
		int sum=0;
		int avg= 0;
		Scanner sc= new Scanner(System.in);
		for (int i = 0; i < m.length; i++) {
			m[i]= sc.nextInt();
		}
		for (int i = 0; i < m.length; i++) {
			if(m[i]<40) {m[i]=40;}
			sum +=m[i]; 
			avg = sum/5;
		}
		System.out.println(avg);
	}

}
